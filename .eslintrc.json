{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "linebreak-style": 0,
        "no-underscore-dangle": 0,
        "no-restricted-globals": 0,
        "import/no-extraneous-dependencies": 0,
        "prefer-destructuring": 0,
        "consistent-return": 0,
        "no-prototype-builtins": 0
    }
}
